{
  "name": "salesmatch-pro",
  "version": "1.0.0",
  "description": "Enterprise B2B Sales Matching Platform for Deep Fon Corporation",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "build:frontend": "cd miniapp/frontend && npm run build",
    "dev:frontend": "cd miniapp/frontend && npm run dev",
    "install:frontend": "cd miniapp/frontend && npm install",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build:all": "npm run build:frontend"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "telegraf": "^4.15.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "telegram-bot",
    "b2b",
